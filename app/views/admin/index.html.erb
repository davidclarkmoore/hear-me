

<% if user_signed_in? %>

	<div class="col-md-12">

	<h1>Welcome Team</h1>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Organizations</h3>
  </div>
  <div class="panel-body">
  <%= link_to 'New Organization', new_organization_path, :class => "btn btn-success right" %>
    <%= render 'organizations/organizationlist' %>
  </div>
</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Exhibits</h3>
  </div>
  <div class="panel-body">
  <%= link_to 'New Exhibit', new_exhibit_path, :class => "btn btn-success right" %>
    <%= render 'exhibits/exhibitlist' %>
  </div>
</div>




	</div>	

<% else %>

	<div class="col-md-4 col-md-offset-4">

	<%= render "users/sessions/signinform" %>

	</div>

<% end %>





